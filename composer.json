{
  "name": "chfur/laravel-remote-data-collector",
  "description": "Provides service for data collecting for chfur/laravel-swagger plugin",
  "keywords": [
    "laravel",
    "swagger",
    "auto-documentation",
    "rest-api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Dubrovin",
      "email": "rdubrovin@ronasit.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "laravel/framework": ">=5.3.0",
    "chfur/laravel-swagger": "dev-master",
    "minime/annotations": "~3.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/CHfur/laravel-swagger"
    }
  ],
  "autoload": {
    "psr-4": {
      "RonasIT\\Support\\DataCollectors\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
